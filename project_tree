import subprocess

class OpenBBWrapper:
    def run_command(self, command: str) -> str:
        try:
            result = subprocess.run(
                command, shell=True, capture_output=True, text=True
            )
            return result.stdout or "‚úÖ Comando eseguito"
        except Exception as e:
            return f"‚ùå Errore: {e}"


